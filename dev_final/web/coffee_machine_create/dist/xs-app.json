{
   "welcomeFile":"/index.html",
   "authenticationMethod":"route",
   "routes":[
      {
         "source":"^(.*)$",
         "target":"$1",
         "service":"html5-apps-repo-rt",
         "csrfProtection":true,
         "authenticationType":"xsuaa",
         "scope":{
            "GET":[
               "$XSAPPNAME.dev.view"
            ],
            "POST":[
               "$XSAPPNAME.dev.edit"
            ],
            "PUT":[
               "$XSAPPNAME.dev.edit"
            ],
            "DELETE":[
               "$XSAPPNAME.dev.edit"
            ],
            "default":"$XSAPPNAME.dev.view"
         }
      }
   ]
}
